{
  "name": "@missingcodec/eslint-plugin",
  "description": "Noa's eslint plugin",
  "version": "1.0.4",
  "author": "Noa <erfan.baghdadi26@gmail.com> (https://github.com/missingcodec/)",
  "homepage": "https://github.com/missingcodec/eslint-plugin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/missingcodec/eslint-plugin.git"
  },
  "license": "MIT",
  "keywords": [
    "javascript",
    "typescript",
    "eslint",
    "eslint-plugin"
  ],
  "type": "module",
  "main": "./dist/index.js",
