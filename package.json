  "devDependencies": {
    "tsdown": "^0.12.9"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "typescript": ">=5.0.0",
    "typescript-eslint": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
